chars = {}
char = 
{
sl = {32,33,32},
id = 1,
pos = {50,50},
ref = 0.05
}

function char:rend()
	spr(self.sl[ceil(self.id)],self.pos[1],self.pos[2])
end

function char:update()
	self.id +=self.ref
	if self.id > #self.sl then
		self.id = 1
	end
end

function drawchars()
	for i=1, #chars do
		chars[i]:rend()
	end
end

function updatechars()
	for i=1, #chars do
		chars[i]:update()
	end
end

