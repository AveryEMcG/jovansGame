 function _init()
 	gamestate = 0
  sprite = {}
  sprite.x = 52
  sprite.y = 90
  sprite.ax = 0
  sprite.ay = 0
  sprite.frame = 1
  sprite.moving = false
  sprite.facingleft = false
  enemylist = {}
  

 
  music(0)
  level = {}
  level.x = {0,15,30,45}
  level.y = {0,0,0,0}
  level.id = 1
  level.messages = {
  "you can do it jovan",
  "this level is hard\nits kind of like your\nmiddle school tests",
  "this level is too hard!\nyou can't beat it\nwithout special tools!\n\ndon't worry, emily\nwill give you the\ntools soon"}
 
 end



 function nextlevel()
 	level.id+=1
 	if level.id == 4 then
  	createnenemy(50,95,30,60,"m1",32,33,1,3)
	 end
	 sprite.x = 52
  sprite.y = 90
  end

 function updateplayer()
  sprite.moving = false
  if btn (0) and sprite.x>=0 then
   sprite.ax-=.3
   sprite.moving = true
  elseif btn(1) and sprite.x<=120 then
   sprite.ax+= .3
   sprite.moving = true
  end
  
  if btnp(5) and on_ground()then
  	sprite.ay= -6
  	sfx(1)
  else
  	sprite.ay +=1
  end
  
  if sprite.ax>2 then
   sprite.ax=2
  elseif sprite.ax <-2 then 
   sprite.ax = -2
  end
  
  if sprite.ax >.1 then
   sprite.facingleft = false
  	sprite.ax-=.1
  elseif sprite.ax <0 then
   sprite.facingleft = true
  	sprite.ax+=.1
  else
   sprite.ax = 0
  end

  if get_collide(sprite.x, sprite.y, sprite.ax, 0)then 
  		sprite.ax = 0
  	else
  	 sprite.x+=sprite.ax
  	end
  	
  	if(get_collide(sprite.x, sprite.y, 0, sprite.ay))then 
  		sprite.ay = 0 
  	else
  		
  		sprite.y +=sprite.ay
  	end

		 if sprite.ax !=0 then
		  sprite.moving = true
		 end
    if sprite.frame >=3 or sprite.moving == false then
      sprite.frame = 1
    else
     sprite.frame+=1
    end

	  if isbell(sprite.x,sprite.y) then
   nextlevel()
	  sfx(3)
	  end
end

function on_ground()
 return get_collide(sprite.x,sprite.y+7,0,0)
end
function get_collide(x,y,ax,ay)

  startx = x + ax
  endx = x + ax+ 7
  
  starty = y + ay
  endy = y+ ay + 7
  
  if (solid(startx,starty)) then
   return true
  end
    
  if (solid(startx,endy)) then
   return true
  end
  
  if (solid(endx, endy)) then
  	return true
  end
    if (solid(endx, starty)) then
  	return true
  end
  return false
  
end


function isbell(x,y)
		x+=level.x[level.id]*8
		y+=level.y[level.id]
  local tilex = ((x-(x % 8) ) / 8)
  local tiley = ((y-(y % 8) ) / 8)
 	if (fget(mget(tilex,tiley) , 1)) then
	 	return true
	 end
	return false
	end

function solid(x,y)
		x+=level.x[level.id] * 8
		y+=level.y[level.id]
  local tilex = ((x-(x % 8) ) / 8)
  local tiley = ((y-(y % 8) ) / 8) 
 	if (fget(mget(tilex,tiley) , 0)) then
 	 return true
 	end 
 	return false
 end

 
 function updateenemies()
 	for e in all(enemylist) do
 		if e.facingleft then
 		 if e.x<=e.xl then
 				e.facingleft = false
 				e.x+=e.speed
 			else 
 			 e.x-=e.speed
 			end
 		else 
 			if e.x>=e.xr then
 				e.facingleft = true
 				e.x-=e.speed
 			else 
 			 e.x+= e.speed
 			end 		

 		end
 		e.frame+=1
 		if e.frame > e.frameend then
 			e.frame = e.framestart
 		end
 		spr(e.frame,e.x,e.y,1,1, e.facingleft)
 end
 end
 
 function _update()
  if gamestate == 1 then
  	updateplayer()
  else
  	updatemsg()
  end
 end
 
 
 
 function createnenemy(x,y,xl,xr,id,framestart,frameend,speed)
 enemy = {}
 enemy.id = id
 enemy.x = x
 enemy.y = y
 enemy.xl = xl
 enemy.xr = xr
 enemy.frame = framestart
 enemy.framestart = framestart
 enemy.frameend = frameend
 enemy.speed = speed
 enemy.facingleft = false
 add (enemylist, enemy)
 return
 end
 
 function updatebackground()
 rectfill(0,0,127,127,0)
 mapdraw(level.x[level.id],0,0,0, 16,16)
 end
 
 function _draw()
  if gamestate ==1 then
 	 updatebackground()
   updateenemies()
	  --draw my new character
  
 	 spr(sprite.frame,sprite.x,sprite.y,1,1, sprite.facingleft)
 	 print("level: "..level.id, 0,0,10)
 	 print (level.messages[level.id],35,0,10)
		else
			drawmsg()
		end
 end
