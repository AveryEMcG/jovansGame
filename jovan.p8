function addchar()

	if added == false then
		if state == 2 then
			c = copy(char)
			
			c.pos = {10,70}
			c.ref = .5
			add(chars,c)
			added = true
		elseif state == 4 then
			c = copy(char)
			c.pos = {100,60}
			c.sl = {4,5,6,7,4}
			c.ref = .1
			add(chars,c)
			added = true

		elseif state == 6 then
			c = copy(char)
			c.pos = {50,80}
			c.sl = {1,2,3,1}
			c.ref = .2
			add(chars,c)
			added = true
		end
	end
end


function btnd(id) 
	if (btn(id)==false) then
		buttond[id] = false
	else
		if buttond[id] then
			return false
		else
		 buttond[id] = true
		 return true
		end
	end
end

function updatemsg()
	if msglist[state] == nil then
		gamestate = 1
	end
	if btnd(4) then
		state +=1
	 added = false
	end
	addchar()
	updatechars()

end

function drawmsg()
	
	cls()
	drawchars()
	print(msglist[state],10,10,10)
	print("press z to continue",10,100,1)
end
